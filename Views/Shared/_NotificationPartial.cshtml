@if (TempData["Success"] != null)
{
    <div class="alert alert-success notification" id="alert"  role="alert">
        @TempData["Success"]
    </div>
}

@if (TempData["Error"] != null)
{
    <div class="alert alert-danger notification " id="alert" role="alert">
        @TempData["Error"]
    </div>
}
